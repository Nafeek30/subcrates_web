</script>
<script type="text/javascript">
	$(document).ready(function(){
		$('.btn_white').click(function(){
			$('.saved_profile').removeClass('active');
			$('.edit_profile').addClass('active');
			$('.btn_white2').addClass('active');
			$('.btn_white').removeClass('active');
		});
		$('.btn_white2').click(function(){
			$('.saved_profile').addClass('active');
			$('.edit_profile').removeClass('active');
			$('.btn_white2').removeClass('active');
			$('.btn_white').addClass('active');
		});
	});
</script>
